library(dplyr)
library(ggplot2)
library(gt)
library(readr)
library(here)
library(htmltools)
dat_clean <- readRDS(here("results", "01_dataclean.Rds"))
glimpse(dat_clean)
overall_summary <- dat_clean %>%
summarise(
n_players = n(),
mean_age = mean(Age),
sd_age = sd(Age),
mean_mp = mean(MP),
sd_mp = sd(MP),
mean_pts = mean(PTS),
sd_pts = sd(PTS)
)
overall_summary %>%
tidyr::pivot_longer(everything()) %>%
gt() %>%
tab_header(title="Overall Summary", subtitle="Age, MP, and PTS")
path_all <- here("results", "02_results", "02_mean&sd_all.html")
